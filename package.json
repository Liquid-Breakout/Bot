{
  "name": "liquid-breakout-backend",
  "version": "0.0.1",
  "description": "The backend serving all requests that requires an external server for Liquid Breakout.",
  "main": "index.js",
  "scripts": {
    "build": "npx babel src --out-dir build --extensions \".js,.ts\" --ignore \"src/**/*.d.ts\"",
    "build_app": "npm run build && bash scripts/build_app.sh",
    "build_app_binary": "node scripts/run_binary_compiler",
    "build_app_balancer": "npm run build_app && cp -f dev_config/balancer.env app/dev_config/.env && npm run build_app_binary -- LBBackend_Balancer",
    "build_app_worker": "npm run build_app && cp -f dev_config/worker.env app/dev_config/.env && npm run build_app_binary -- LBBackend_Worker",
    "start": "npm run build && node build/index.js",
    "start:dev": "npx nodemon"
  },
  "author": "cutymeo",
  "license": "ISC",
  "dependencies": {
    "@wasm-audio-decoders/flac": "^0.1.12",
    "@wasm-audio-decoders/ogg-vorbis": "^0.1.7",
    "axios": "^0.26.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "consola": "^3.2.3",
    "discord.js": "^14.12.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fft.js": "^4.0.4",
    "geolite2-redist": "^3.0.4",
    "guid-typescript": "^1.0.9",
    "maxmind": "^4.3.11",
    "meyda": "^5.6.2",
    "mongoose": "^7.2.4",
    "mpg123-decoder": "^0.4.8",
    "nmap": "^1.1.0",
    "node-fetch": "^3.3.1",
    "node-wav": "^0.0.2",
    "ogg-opus-decoder": "^1.6.4",
    "public-ip": "^6.0.1",
    "qoa-format": "^1.0.0",
    "saslprep": "^1.0.3",
    "sockjs": "^0.3.24",
    "sockjs-client": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/meyda": "^4.3.3",
    "@types/node": "^18.14.0",
    "@types/node-fetch": "^2.6.4",
    "@types/sockjs": "^0.3.33",
    "@types/sockjs-client": "^1.5.1",
    "are-you-es5": "*",
    "babel-loader": "^9.1.3",
    "depd": "^2.0.0",
    "nexe": "^4.0.0-rc.2",
    "node": "^16.10.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "yn": "^5.0.0"
  },
  "keywords": []
}
